{
  "name": "@desuyume/eslint",
  "description": "eslint config",
  "type": "module",
  "version": "1.0.0",
  "license": "ISC",
  "keywords": [
    "eslint"
  ],
  "author": "desuyume (https://github.com/desuyume)",
  "homepage": "https://github.com/desuyume/core",
  "repository": {
    "url": "git+https://github.com/desuyume/core.git"
  },
  "bugs": {
    "url": "https://github.com/desuyume/core/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "format": "prettier --write \"*.js\"",
    "lint": "eslint . --fix",
    "lint-inspector": "npx @eslint/config-inspector"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "eslint": "^9.9.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.25.1",
    "@eslint-react/eslint-plugin": "^1.10.0",
    "eslint": "^9.9.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-simple-import-sort": "^12.1.1"
  },
  "devDependencies": {
    "@desuyume/prettier": "*"
  }
}
